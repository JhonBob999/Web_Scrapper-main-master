  <property name="styleSheet">
   <string notr="true">/* Основное окно и вкладки */
QMainWindow {
    background-color: #f4f7f6;
    color: #222831;
    font-family: 'Segoe UI', sans-serif;
}

QTabWidget::pane {
    border: none;
    background-color: #ffffff;
}

QTabBar::tab {
    background-color: #ffffff;
    color: #222831;
    padding: 8px 15px;
    border: 1px solid #dddddd;
    border-radius: 6px;
    margin-right: 4px;
}

QTabBar::tab:selected {
    background-color: #00adb5;
    color: #ffffff;
    border: none;
}

/* Кнопки */
QPushButton {
    background-color: #00adb5;
    color: white;
    padding: 6px 15px;
    border-radius: 6px;
}

QPushButton:hover {
    background-color: #028a91;
}

/* Поля ввода */
QLineEdit, QTextEdit {
    background-color: #ffffff;
    color: #222831;
    border: 1px solid #dddddd;
    border-radius: 5px;
    padding: 4px;
}

/* Таблица и списки */
QTableWidget, QListWidget {
    background-color: #ffffff;
    color: #222831;
    gridline-color: #f0f0f0;
    border: 1px solid #dddddd;
    border-radius: 6px;
}

/* Логи */
QTextBrowser {
    background-color: #ffffff;
    color: #222831;
    border: 1px solid #dddddd;
    border-radius: 6px;
}

/* ===== ДОПОЛНЕНИЯ ===== */

/* Label (подписи и текст) */
QLabel {
    color: #393e46;
    font-size: 18px;
}

/* CheckBox */
QCheckBox {
    color: #222831;
    padding: 3px;
    spacing: 5px;
}

QCheckBox::indicator {
    width: 16px;
    height: 16px;
    border-radius: 3px;
    border: 1px solid #00adb5;
    background-color: #ffffff;
}

QCheckBox::indicator:checked {
    background-color: #00adb5;
    border: none;
}

/* ComboBox */
QComboBox {
    background-color: #ffffff;
    color: #222831;
    border: 1px solid #dddddd;
    border-radius: 5px;
    padding: 4px;
}

QComboBox::drop-down {
    border: none;
}

QComboBox QAbstractItemView {
    background-color: #ffffff;
    selection-background-color: #00adb5;
    selection-color: #ffffff;
    border-radius: 5px;
    padding: 3px;
}

QToolBar::separator {
    background-color: #333333;  /* Тёмно-серый / почти чёрный */
    width: 2px;
    margin: 4px;
}
</string>
